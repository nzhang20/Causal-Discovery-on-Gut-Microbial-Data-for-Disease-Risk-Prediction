	journal = {bioRxiv},
	publisher = {Cold Spring Harbor Laboratory},
	title = {BIRDMAn: A Bayesian differential abundance framework that enables robust inference of host-microbe associations},
	url = {https://www.biorxiv.org/content/early/2023/02/02/2023.01.30.526328},
	year = {2023},
	bdsk-url-1 = {https://www.biorxiv.org/content/early/2023/02/02/2023.01.30.526328},
